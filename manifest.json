{
  "manifest_version": 2,
  "name": "wikim",
  "description": "Redirects from Wikipedia articles to their mobile version.",
  "author": "Brian Black",
  "version": "1",
  "permissions": [
    "https://*.wikipedia.org/wiki/*"
  ],
  "content_scripts": [
    {
      "run_at": "document_start",
      "matches": [
        "https://*.wikipedia.org/wiki/*"
      ],
      "js": [
        "script.js"
      ]
    }
  ]
}
